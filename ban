game:GetService("UserInputService").InputBegan:connect(
    function(key, gpe)
        if key.KeyCode == Enum.KeyCode.RightControl then
            pcall(
                function()
                    for i, v in pairs(game.CoreGui["Ban Menu (With many Other UI's)"]:GetChildren()) do
                        v.Visible = not v.Visible
                    end
                end
            )
        end
    end
)







