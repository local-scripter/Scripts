local FirstLabelYay = SecondPage.AddLabel("Not Made By Me Only UI LIB Is Mine :,( Ban Menu Made by LosSantosDev#0901")

game:GetService("UserInputService").InputBegan:connect(
    function(key, gpe)
        if key.KeyCode == Enum.KeyCode.RightControl then
            pcall(
                function()
                    for i, v in pairs(game.CoreGui["What's this"]:GetChildren()) do
                        v.Visible = not v.Visible
                    end
                end
            )
        end
    end
)
