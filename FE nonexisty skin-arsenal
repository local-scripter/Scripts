local a = 1

function zigzag(X)
    return math.acos(math.cos(X * math.pi)) / math.pi
end

game:GetService("RunService").RenderStepped:Connect(function()
    if game.Workspace.Camera:FindFirstChild('Arms') then
        wait(1)
        for i,v in pairs(game.Workspace.Camera.Arms:GetDescendants()) do
            if v.ClassName == 'MeshPart' then
                v.Color = Color3.fromHSV(zigzag(a),1,1)
                a = a + .0001
            end
        end
    end
end)

function monke()
    wait(1)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/local-scripter/Scripts/main/ban"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/local-scripter/Scripts/main/Monke's%20ESP"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/local-scripter/Scripts/main/Aimbot"))()
end

monke()

